    "@types/selenium-webdriver": "^2.52.0",
    "autoprefixer": "7.1.2",
    "codelyzer": "4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~5.0.1",
    "tslint": "5.9.1",
    "typescript": "~3.1.3"
  }
}
